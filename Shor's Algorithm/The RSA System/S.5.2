def decode(d, N, code):
    """Decode an encrypted message

    Args:
        d (int): Value of the RSA codification.
        N (int): Product of p and q.
        code list[int]: List of values to be decoded.

    Returns:
        string: Decoded message. (One character per list item)
    """

    ##################
    # YOUR CODE HERE #
    ##################
    new_message = []

    for number in code:
        decrypted_number = number**d % N
        decrypted_message = chr(decrypted_number)
        new_message.append(decrypted_message)

    message = ''.join(str(character) for character in new_message)
        

    return message


