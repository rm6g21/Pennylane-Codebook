dev = qml.device("default.qubit", wires=1)

U = np.array([[1, 1], [1, -1]]) / np.sqrt(2)


@qml.qnode(dev)
def apply_u():

    ##################
    # YOUR CODE HERE #
    ##################
    qml.QubitUnitary(U,wires=0)
    # USE QubitUnitary TO APPLY U TO THE QUBIT

    # Return the state
    return qml.state()
